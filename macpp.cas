protocol MACPP;
identifiers
KGC, Va, Vb, OBU	: user;          
G, Timestamp, M, Tag, ADj, Ka, W   		: number;
H1, H2, H3	: function;


messages
1. KGC -> Vb : KGC, G^Tag
2. KGC -> OBU :  KGC, Va, ADj, Tag
3. OBU -> Va : OBU, Va, G^Ka, H1(Va, G^Tag^Ka), H2(Ka, G^Ka,Timestamp), Timestamp
4. Va -> Vb :  Va, M, G^Ka, H1(Va, G^Tag^Ka, ADj), H3(G^W, M, ADj, Va, Timestamp), ADj, G^W, H2(G^Ka,Timestamp),H3(G^W, M, ADj, Va, Timestamp)^W,Timestamp 

knowledge
Va	: KGC, Va, Vb, Timestamp, Ka, W;        
Vb	: KGC, Va, Vb;        
KGC	: Va, Vb;
OBU : KGC, Va, Vb, Timestamp, Ka, W;


%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
session_instances
[KGC: kgc, Va: va, Vb: vb, OBU: obu];     

intruder_knowledge
 kgc, va, vb;                 


goal
   Vb authenticates Va on W;
   Vb authenticates Va on Timestamp;